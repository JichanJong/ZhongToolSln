<Window x:Class="ZhongTool.FrmCodeViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZhongTool"
        mc:Ignorable="d"
        Title="FrmCodeViewer" Height="300" Width="300" Loaded="FrmCodeViewer_Loaded">
    <Grid>
        <TextBox TextWrapping="Wrap" AcceptsReturn="True" x:Name="txtCode"></TextBox>
    </Grid>
</Window>
